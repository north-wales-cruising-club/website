{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
        "type": "package",
        "package": {
            "name": "wordpress/wordpress",
			"replace": {
				"johnpbloch/wordpress": "*"
			},
            "type": "wordpress-core",
            "version": "4.6.1",
            "source": {
                "type": "git",
                "url": "https://github.com/WordPress/WordPress.git",
                "reference": "4.6.1"
            }
        }
    }
  ],
  "require": {
    "wordpress/wordpress": "*",
    "wpackagist-plugin/akismet": "3.2",
    "wpackagist-plugin/buddypress": "2.6.2",
    "wpackagist-plugin/cimy-user-extra-fields": "2.7.1",
    "wpackagist-plugin/cimy-user-manager": "1.5.0",
    "wpackagist-plugin/cookie-notice": "1.2.36.1",
    "wpackagist-plugin/event-organiser": "3.1.4",
    "wpackagist-plugin/jetpack": "4.3.1",
    "wpackagist-plugin/netcam-live-stream": "1.0.5",
    "wpackagist-plugin/oik": "3.0.2",
    "wpackagist-plugin/the-events-calendar": "4.2.7",
    "wpackagist-plugin/theme-my-login": "6.4.5",
    "wpackagist-plugin/uk-tides": "1.6.3",
    "wpackagist-plugin/vertical-news-scroller": "1.3.1",
    "wpackagist-plugin/visual-form-builder": "2.9.2",
    "wpackagist-plugin/wordpress-seo": "3.6",
    "wpackagist-plugin/wp-forecast": "5.5",
    "wpackagist-theme/smartline-lite": "1.4.2",
	"psycle-wordpress/wordpress-core-installer": "^1.0",
    "psycle-wordpress-plugins/mail-options": "^1.0"
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
        "custom/mu-plugins/{$name}": ["type:wordpress-muplugin"],
        "custom/plugins/{$name}/": ["type:wordpress-plugin"],
        "custom/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
