{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
        "type": "package",
        "package": {
            "name": "wordpress/wordpress",
			"replace": {
				"johnpbloch/wordpress": "*"
			},
            "type": "wordpress-core",
            "version": "4.7.1",
            "source": {
                "type": "git",
                "url": "https://github.com/WordPress/WordPress.git",
                "reference": "4.7.1"
            }
        }
    }
  ],
  "require": {
    "wordpress/wordpress": "*",
    "wpackagist-plugin/akismet": "3.2",
    "wpackagist-plugin/awesome-weather": "1.5.10",
    "wpackagist-plugin/buddypress": "2.7.4",
    "wpackagist-plugin/cookie-notice": "1.2.37",
    "wpackagist-plugin/events-manager": "5.6.6.1",
    "wpackagist-plugin/import-users-from-csv-with-meta": "1.9.9.8",
    "wpackagist-plugin/jetpack": "4.4.2",
    "wpackagist-plugin/mailgun": "1.5.7",
    "wpackagist-plugin/netcam-live-stream": "1.0.5",
    "wpackagist-plugin/restrict-content-by-role": "3.5.0",
    "wpackagist-plugin/theme-my-login": "6.4.7",
    "wpackagist-plugin/user-role-editor": "4.31.1",
    "wpackagist-plugin/visual-form-builder": "2.9.2",
    "wpackagist-plugin/wordpress-seo": "4.1",
    "wpackagist-theme/smartline-lite": "1.4.3",
	"psycle-wordpress/wordpress-core-installer": "^1.0"
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
        "custom/mu-plugins/{$name}": ["type:wordpress-muplugin"],
        "custom/plugins/{$name}/": ["type:wordpress-plugin"],
        "custom/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
